@model List<List<string>>
@{
    ViewBag.Title = "Generated TimeTable";
    var workingDays = ViewBag.WorkingDays as int?;
}
<h2>Final TimeTable</h2>
<table class="table table-bordered">
    <thead>
        
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
               
                @foreach (var subject in Model[i])
                {
                    <td>@subject</td>
                }
            </tr>
        }
    </tbody>
</table>
<a href="@Url.Action("Index", "TimeTable")" class="btn btn-secondary">Back to Home</a>
